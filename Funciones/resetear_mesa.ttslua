    bloque_canasta_tipo_E2_C07 = getObjectFromGUID(bloque_canasta_tipo_E2_C07_GUID)
    bloque_canasta_tipo_E2_C08 = getObjectFromGUID(bloque_canasta_tipo_E2_C08_GUID)

    if ronda >= 1 then

    -- PONEMOS PUNTOS DE CONTADORES DE CANASTAS A CERO
    contador_puntos_canastas_E1.setValue(0)
    contador_puntos_canastas_E2.setValue(0)

    elseif ronda == 0 then

      -- PONEMOS PUNTOS DE TODOS LOS CONTADORES A CERO
      contador_puntos_canastas_E1.setValue(0)
      contador_puntos_canastas_E2.setValue(0)
      contador_puntos_total_E1.setValue(0)
      contador_puntos_total_E2.setValue(0)

    end

    -- PASAMOS DE RONDA
    contador_ronda.setValue(ronda+1)

    -- RESETEAMOS COLORES DE BASES DE CANASTA Y TEXTOS
    for i = 1,2 do
      for j = 1,8 do
        bloque = 'bloque_canasta_E' .. i .. '_C0' .. j
        bloque_tipo = 'bloque_canasta_tipo_E' .. i .. '_C0' .. j
        _G[bloque].setColorTint({0.7,0.7,0.7})
        _G[bloque_tipo].setColorTint({0.7,0.7,0.7})
      end
    end

    for i = 1,2 do
      for j = 1,9 do
        txt_gui = 'canasta_tipo_E' .. i .. '_C0' .. j
        _G[txt_gui].setValue(' ')
      end
    end

  end


end

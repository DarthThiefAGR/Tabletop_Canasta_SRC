    contador_puntos_total_E2 = getObjectFromGUID(contador_puntos_total_E2_GUID)

    -- CONTADOR RONDA
    contador_ronda = getObjectFromGUID(contador_ronda_GUID)

    -- TEXTOS CANASTA EQUIPO 1
